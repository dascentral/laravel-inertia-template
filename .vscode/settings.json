{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "editor.renderFinalNewline": "dimmed",
  "laravel-pint.enable": true,
  "yaml.customTags": ["!Ref"],
  "[blade]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "[vue]": {
    "editor.formatOnSave": false
  }
}
